{
  "name": "ts-publish",
  "version": "0.0.0",
  "description": "Typescript project publishing workflow.",
  "main": "index.js",
  "typings": "index.d.ts",
  "bin": {
    "ts-pre-release": "./bin/ts-pre-release.js",
    "ts-release": "./bin/ts-release.js",
    "ts-compile": "./bin/ts-compile.js"
  },
  "scripts": {
    "build": "tsc",
    "pre-release": "tsc && node ./build/ts-pre-release.js ./build/_hook.js",
    "release": "tsc && node ./build/ts-release.js ./build/_hook.js"
  },
  "keywords": [
    "typescript",
    "publish",
    "release",
    "pre-release",
    "pre-publish"
  ],
  "dependencies": {
    "@types/colors": "^0.6.33",
    "@types/lodash": "^4.14.37",
    "@types/node": "^6.0.45",
    "@types/yargs": "^0.0.33",
    "colors": "^1.1.2",
    "lodash": "^4.16.4",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "yargs": "^6.2.0"
  }
}
